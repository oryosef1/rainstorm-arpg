{
  "name": "agent-dashboard",
  "version": "1.0.0",
  "description": "Revolutionary AI development dashboard with Claude Code orchestration",
  "main": "dist/agent-dashboard.pod.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch agent-dashboard.pod.ts",
    "build": "tsc",
    "start": "node dist/agent-dashboard.pod.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "docker:build": "docker build -f docker/Dockerfile -t agent-dashboard:latest .",
    "docker:dev": "docker-compose -f docker/docker-compose.yml up -d",
    "k8s:deploy": "kubectl apply -f kubernetes/deployment.yaml"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@prisma/client": "^5.7.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "monaco-editor": "^0.45.0",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "recharts": "^2.8.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.4.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "ai",
    "dashboard",
    "claude",
    "automation",
    "development",
    "typescript",
    "react",
    "workflow"
  ],
  "author": "Agent Dashboard Team",
  "license": "MIT"
}
