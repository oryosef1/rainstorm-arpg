{
  "source": {
    "include": [
      "./game-core/",
      "./README.md"
    ],
    "includePattern": "\\.(js)$",
    "exclude": [
      "node_modules/",
      "dist/",
      "coverage/",
      "tests/",
      "**/*.test.js"
    ]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}